{
  "name": "zagovorichev/composer-smart-installer",
  "description": "Correct folders installation",
  "keywords": ["composer"],
  "license": "MIT",
  "type": "composer-plugin",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1",
    "composer-plugin-api": "^1.1 || ^2.0",
    "composer/installers": "^1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "composer/composer": "^2.0",
    "phpunit/phpunit": "^9.5.2",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Zagovorichev\\Composer\\": "composer/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zagovorichev\\Composer\\Tests\\": "tests/"
    }
  },
  "extra": {
    "class": "Zagovorichev\\Composer\\Installers\\Plugin"
  },
  "authors": [
    {
      "name": "Oleksandr Zagovorychev",
      "email": "zagovorichev@gmail.com"
    }
  ]
}
